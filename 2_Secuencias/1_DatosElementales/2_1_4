ACCION Eje_2_1_4 ES
    Ambiente
        Sec : Secuencia de Entero
        socio, cont : Entero

        PROCEDIMIENTO Recorrer_secuencia ES
            MIENTRAS NFDS(Sec_entrada) HACER
                cont := cont + 1
                AVZ(Sec_entrada, car)
            FIN_MIENTRAS
        FIN_PROCEDIMIENTO

        PROCEDIMIENTO Mostrar_Resultados ES
            ESCRIBIR("cantidad de socios: ", cont)
        FIN_PROCEDIMIENTO

        PROCEDIMIENTO Inicializar ES
            ARR(Sec_entrada)
            AVZ(Sec_entrada, socio)
            cont := 0
        FIN_PROCEDIMIENTO

        PROCEDIMIENTO Finalizar ES
            CERRAR(Sec_entrada)
        FIN_PROCEDIMIENTO
    Proceso
        Inicializar
        Recorrer_secuencia
        Mostrar_Resultados
        Finalizar
FIN_ACCION