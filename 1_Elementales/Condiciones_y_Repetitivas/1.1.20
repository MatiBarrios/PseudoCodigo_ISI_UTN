ACCION Eje_1_1_19 ES
    Ambiente
        v, aux : Entero
    Proceso
        ESCRIBIR("ingrese el valor")
        LEER(v)

        PARA i := 1 HASTA 10 HACER
            aux := (v + (i - 1))
            ESCRIBIR(aux)
            aux := (v + (i - 1)) ** 2
            ESCRIBIR(aux)
            aux := (v + (i - 1)) ** 3
            ESCRIBIR(aux)
        FIN_PARA

FIN_ACCION